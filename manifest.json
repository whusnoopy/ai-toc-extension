{
  "manifest_version": 3,
  "name": "AI Chat TOC Pro",
  "version": "2.6.0",
  "description": "具有精致 UI 和渐变图标的 AI 聊天目录导航",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*",
        "https://gemini.google.com/*",
        "https://claude.ai/*"
      ],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ]
}